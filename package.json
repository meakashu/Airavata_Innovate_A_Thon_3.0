{
  "name": "trustbridge-protocol",
  "version": "1.0.0",
  "description": "Fully Decentralized Healthcare Data Exchange Platform",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:accessibility": "axe-core",
    "test:contracts": "cd contracts && npx hardhat test",
    "test:coverage": "jest --coverage",
    "test:all": "npm test && npm run test:contracts && npm run test:e2e",
    "test:wallet": "jest --testPathPattern=wallet",
    "test:storage": "jest --testPathPattern=storage",
    "test:payments": "jest --testPathPattern=payments",
    "deploy:fast": "npm install && npm run setup:fast && npm run deploy:contracts && npm run update:addresses && npm run dev",
    "setup:fast": "node scripts/setup-fast.js",
    "setup:env": "node scripts/setup-env.js",
    "reset": "rm -f .env.local && npm run setup:fast",
    "clear:cache": "rimraf .next && rimraf node_modules/.cache",
    "rebuild:contracts": "cd contracts && npx hardhat clean && npx hardhat compile",
    "update:addresses": "node scripts/update-addresses.js",
    "deploy:all": "npm run deploy:contracts && npm run deploy:frontend && npm run deploy:functions",
    "deploy:production": "npm run build && npm run deploy:lighthouse",
    "deploy:lighthouse": "npm run build && fleek deploy",
    "deploy:vercel": "npm run build && vercel --prod",
    "deploy:contracts": "cd contracts && npx hardhat deploy --network base",
    "deploy:contracts:mainnet": "cd contracts && npx hardhat deploy --network base",
    "deploy:contracts:testnet": "cd contracts && npx hardhat deploy --network baseSepolia",
    "deploy:frontend": "npm run build && npm run deploy:lighthouse",
    "deploy:functions": "cd edge && npm run deploy",
    "ceramic:start": "docker-compose -f docker-compose.ceramic.yml up -d",
    "ceramic:stop": "docker-compose -f docker-compose.ceramic.yml down",
    "ceramic:restart": "npm run ceramic:stop && npm run ceramic:start",
    "ceramic:deploy": "node scripts/deploy-ceramic.js",
    "ceramic:dev": "cross-env NODE_ENV=development npm run ceramic:start",
    "ceramic:prod": "cross-env NODE_ENV=production npm run ceramic:start",
    "ceramic:status": "npx @ceramicnetwork/cli status",
    "ceramic:test": "node scripts/test-ceramic.js",
    "lighthouse:ci": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "status": "node scripts/check-status.js",
    "logs": "node scripts/view-logs.js",
    "monitor:tx": "node scripts/monitor-transactions.js",
    "restart": "npm run stop && npm run dev",
    "stop": "pkill -f 'next dev'",
    "postinstall": "npm run install:subgraph && npm run install:edge",
    "install:subgraph": "cd subgraph && npm install",
    "install:edge": "cd edge && npm install"
  },
  "dependencies": {
    "@3id/connect": "^0.4.1",
    "@ceramicnetwork/http-client": "^2.36.0",
    "@ceramicnetwork/stream-tile": "^2.35.0",
    "@chainlink/contracts": "^0.8.0",
    "@chakra-ui/icons": "^2.1.0",
    "@chakra-ui/react": "^2.8.0",
    "@coinbase/cdp-sdk": "^1.36.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@glazed/datamodel": "^0.3.1",
    "@glazed/did-datastore": "^0.3.2",
    "@libp2p/bootstrap": "^9.0.0",
    "@libp2p/mplex": "^8.0.0",
    "@libp2p/noise": "^12.0.1",
    "@libp2p/peer-id": "^2.0.0",
    "@libp2p/pubsub": "^8.0.0",
    "@libp2p/websockets": "^9.2.19",
    "@nftstorage/ipfs-cluster": "^5.0.1",
    "@oasisprotocol/client": "^1.3.0",
    "@openzeppelin/contracts-upgradeable": "^5.4.0",
    "@spruceid/didkit-wasm": "^0.3.0-rc.0",
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@tensorflow/tfjs": "^4.15.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@walletconnect/web3-provider": "^1.8.0",
    "arweave": "^1.0.0",
    "assert": "^2.0.0",
    "axios": "^1.11.0",
    "browserify-zlib": "^0.2.0",
    "circomlibjs": "^0.1.7",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "date-fns": "^2.30.0",
    "did-jwt": "^5.0.0",
    "dids": "^2.4.3",
    "ethers": "^6.8.0",
    "framer-motion": "^10.16.0",
    "helia": "^1.0.0",
    "https-browserify": "^1.0.0",
    "ipfs-core": "^0.18.1",
    "ipfs-http-client": "^60.0.0",
    "key-did-provider-ed25519": "^2.0.1",
    "key-did-resolver": "^2.3.0",
    "libp2p": "^0.40.0",
    "next": "^14.0.0",
    "next-pwa": "^5.6.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-error-boundary": "^4.0.0",
    "react-helmet-async": "^2.0.0",
    "react-hook-form": "^7.48.0",
    "react-hot-toast": "^2.4.0",
    "react-icons": "^4.12.0",
    "recharts": "^2.8.0",
    "snarkjs": "^0.7.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "typescript": "^5.0.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@openzeppelin/contracts": "^5.4.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/chai": "^4.3.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "axe-core": "^4.7.0",
    "chai": "^4.3.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "hardhat": "^2.19.0",
    "husky": "^8.0.0",
    "jest": "^29.0.0",
    "lint-staged": "^15.0.0",
    "playwright": "^1.40.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "rimraf": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "@tensorflow/tfjs": "^4.15.0",
    "libp2p": "^0.40.0"
  }
}
